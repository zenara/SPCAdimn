        </th>
        <th>
            @Html.DisplayNameFor(model => model.version.v_id)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.policy.p_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.user.u_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.version.v_id)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ac_id }) |
